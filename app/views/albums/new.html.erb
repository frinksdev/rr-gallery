<h1>Nuevo album</h1>
<hr>
<%= form_for @album, url: albums_path, html: { multipart: true } do |form| %>
<div>
    <%= form.label :title %>
    <%= form.text_field :title %>
</div>
<div>
    <%= form.label :images %>
    <%= form.file_field :images %>
</div>
<div>
    <%= form.submit %>
</div>
<% end %>